package me.lukewalker.plugin.entities;

import me.lukewalker.sandbox.entities.Entity;
import me.lukewalker.sandbox.entities.type.EntityType;

public class EntityFragger extends EntityType implements me.lukewalker.sandbox.entities.type.LivingEntity {

	@Override
	public String getName() {
		return "Fragger";
	}

	@Override
	public String getSprite() {
		return "fragger.png";
	}

	@Override
	public int getSpeed() {
		return 10;
	}
	
	@Override
	public void move(Entity ent, String direction, int steps) {
		
	}
}
